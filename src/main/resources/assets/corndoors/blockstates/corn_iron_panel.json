{
    "multipart": [
        {
            "when":{
                "facing":"south",
                "type":"double"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel"
            }
        },
        {
            "when":{
                "facing":"west",
                "type":"double"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":90
            }
        },
        {
            "when":{
                "facing":"north",
                "type":"double"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":180
            }
        },
        {
            "when":{
                "facing":"east",
                "type":"double"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":270
            }
        },


        {
            "when":{
                "facing":"south",
                "type":"top"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_top"
            }
        },
        {
            "when":{
                "facing":"west",
                "type":"top"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_top",
                "y":90
            }
        },
        {
            "when":{
                "facing":"north",
                "type":"top"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_top",
                "y":180
            }
        },
        {
            "when":{
                "facing":"east",
                "type":"top"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_top",
                "y":270
            }
        },


        {
            "when":{
                "facing":"south",
                "type":"bottom"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_bottom"
            }
        },
        {
            "when":{
                "facing":"west",
                "type":"bottom"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_bottom",
                "y":90
            }
        },
        {
            "when":{
                "facing":"north",
                "type":"bottom"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_bottom",
                "y":180
            }
        },
        {
            "when":{
                "facing":"east",
                "type":"bottom"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_glass_bottom",
                "y":270
            }
        },



        {
            "when":{
                "facing":"north",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite"
            }
        },
        {
            "when":{
                "facing":"east",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":90
            }
        },
        {
            "when":{
                "facing":"south",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":180
            }
        },
        {
            "when":{
                "facing":"west",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":270
            }
        }
    ]
}
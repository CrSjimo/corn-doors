{
    "multipart": [
        {
            "when":{
                "facing":"south"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel"
            }
        },
        {
            "when":{
                "facing":"west"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":90
            }
        },
        {
            "when":{
                "facing":"north"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":180
            }
        },
        {
            "when":{
                "facing":"east"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel",
                "y":270
            }
        },



        {
            "when":{
                "facing":"north",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite"
            }
        },
        {
            "when":{
                "facing":"east",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":90
            }
        },
        {
            "when":{
                "facing":"south",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":180
            }
        },
        {
            "when":{
                "facing":"west",
                "near_andesite":"true"
            },
            "apply":{
                "model":"corndoors:block/corn_iron_panel_near_andesite",
                "y":270
            }
        }
    ]
}